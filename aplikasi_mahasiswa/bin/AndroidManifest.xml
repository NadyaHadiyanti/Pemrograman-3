    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.praktikum.mahasiswa.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.praktikum.mahasiswa.List_item_mhs"
            android:label="@string/title_activity_list_item_mhs" >
        </activity>
        <activity
            android:name="com.praktikum.mahasiswa.Mahasiswa"
            android:label="@string/title_activity_mahasiswa" >
        </activity>
    </application>

</manifest>
